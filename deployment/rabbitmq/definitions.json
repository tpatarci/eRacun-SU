{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "users": [
    {
      "name": "eracun",
      "password_hash": "v8TxBJWzRW+QCdnJT0bPfqZ0WoNPsQx7z3F3PzE3QWU=",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "eracun",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "eracun-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "DLX",
      "pattern": "^(?!dead-letter).*",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "dead-letter-exchange",
        "dead-letter-routing-key": "dead-letter"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "fiscalization.requests",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "fiscalization.responses",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "dead-letter",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-message-ttl": 259200000
      }
    }
  ],
  "exchanges": [
    {
      "name": "dead-letter-exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "dead-letter-exchange",
      "vhost": "/",
      "destination": "dead-letter",
      "destination_type": "queue",
      "routing_key": "dead-letter",
      "arguments": {}
    }
  ]
}
